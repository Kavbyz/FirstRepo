@model IEnumerable<Store.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Пользователи</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @*@Html.ActionLink("Details", "Index", "Manage", routeValues: new { UserId = item.Id }, htmlAttributes: null)*@
            @Html.ActionLink("Редактировать", "AddPhoneNumber", "Manage", routeValues: new { UserId = item.Id }, htmlAttributes: null)|
            @Html.ActionLink("Удалить", "Delete", new { id=item.Id })
            @*@Html.ActionLink("Удалить", "Delete", "Account", new { id = (string)item.Id })*@
        </td>
    </tr>
}

</table>
